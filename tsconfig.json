{
  "compilerOptions": {
    "target": "ES5",
    "lib": ["es2015", "dom"],
    "module": "es2015",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noEmitOnError": true,
    "sourceMap": true
  },
  "compileOnSave": false,
  "exclude": [
    "demo",
    "node_modules",
    "compiled"
  ],
  "angularCompilerOptions": {
    "genDir": "compiled"
  }
}
